<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigat"
    app:startDestination="@id/initialFragment">

    <fragment
        android:id="@+id/homeeFragment"
        android:name="com.nfg.yemektarifi.HomeeFragment"
        android:label="HomeeFragment" >
        <action
            android:id="@+id/action_homeeFragment_to_onlyCorba"
            app:destination="@id/onlyCorba"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_to_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right" />
        <action
            android:id="@+id/action_homeeFragment_to_onlyYemek"
            app:destination="@id/onlyYemek"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_to_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right"/>
        <action
            android:id="@+id/action_homeeFragment_to_onlyTatli"
            app:destination="@id/onlyTatli"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_to_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right"/>
        <action
            android:id="@+id/action_homeeFragment_to_yemekTatli"
