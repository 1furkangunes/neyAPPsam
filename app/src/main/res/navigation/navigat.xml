            app:exitAnim="@anim/exit_to_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right"/>
        <action
            android:id="@+id/action_homeeFragment_to_allcyt"
            app:destination="@id/allcyt"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_to_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right"/>
        <action
            android:id="@+id/action_homeeFragment_to_corbaTatli"
            app:destination="@id/corbaTatli"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_to_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right"/>
        <action
            android:id="@+id/action_homeeFragment_to_corbaYemek"
            app:destination="@id/corbaYemek"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_to_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right"/>
    </fragment>
    <fragment
        android:id="@+id/onlyCorba"
        android:name="com.nfg.yemektarifi.OnlyCorba"
        android:label="fragment_only_corba"
        tools:layout="@layout/fragment_only_corba" >
        <action
            android:id="@+id/action_onlyCorba_to_recipeDetailFragment"
            app:destination="@id/recipeDetailFragment"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_to_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right" />
    </fragment>
    <fragment
        android:id="@+id/onlyYemek"
        android:name="com.nfg.yemektarifi.OnlyYemek"
        android:label="fragment_only_yemek"
        tools:layout="@layout/fragment_only_yemek" >
