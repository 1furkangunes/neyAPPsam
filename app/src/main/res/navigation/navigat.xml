            app:exitAnim="@anim/exit_to_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right"/>
    </fragment>
    <fragment
        android:id="@+id/yemekTatli"
        android:name="com.nfg.yemektarifi.YemekTatli"
        android:label="fragment_yemek_tatli"
        tools:layout="@layout/fragment_yemek_tatli" >
        <action
            android:id="@+id/action_yemekTatli_to_recipeDetailFragment"
            app:destination="@id/recipeDetailFragment"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_to_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right"/>
    </fragment>
    <fragment
        android:id="@+id/allcyt"
        android:name="com.nfg.yemektarifi.Allcyt"
        android:label="fragment_allcyt"
        tools:layout="@layout/fragment_allcyt" >
        <action
            android:id="@+id/action_allcyt_to_recipeDetailFragment"
            app:destination="@id/recipeDetailFragment"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_to_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right"/>
    </fragment>
    <fragment
        android:id="@+id/recipeDetailFragment"
        android:name="com.nfg.yemektarifi.RecipeDetailFragment"
        android:label="fragment_recipe_detail"
        tools:layout="@layout/fragment_recipe_detail" />
    <fragment
        android:id="@+id/initialFragment"
        android:name="com.nfg.yemektarifi.InitialFragment"
        android:label="fragment_initial"
        tools:layout="@layout/fragment_initial" >
        <action
            android:id="@+id/action_initialFragment_to_homeeFragment"
            app:destination="@id/homeeFragment" />
    </fragment>
</navigation>